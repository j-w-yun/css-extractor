{
    "name": "CSS Extractor",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "clipboardWrite"
    ],
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "action": {
        "default_icon": "icons/16.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
